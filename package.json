    "js-data": "^2.9.0",
    "moment": "^2.12.0",
    "payment-license": "https://github.com/emschwartz/payment-license.git",
    "uuid": "^2.0.1",
    "webtorrent": "https://github.com/emschwartz/webtorrent.git",
    "wt_ilp": "https://github.com/emschwartz/wt_ilp.git"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
